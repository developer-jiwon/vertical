{
  "expo": {
    "name": "Vertical Calendar",
    "slug": "vertical-calendar-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "verticalcalendar",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#D6E2E0",
      "resizeMode": "contain",
      "image": "./assets/images/custom/splash.png"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wonnie.verticalcalendar",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#D6E2E0"
      },
      "package": "com.wonnie.verticalcalendar"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "updates": {
      "enabled": false
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bc70b7cb-a264-472e-8751-9dca5c1dae77"
      }
    },
    "owner": "wonnie"
  }
}
